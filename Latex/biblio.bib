@article{vilensky_trot-gallop_1991,
	title = {Trot-{Gallop} {Gait} {Transitions} in {Quadrupeds}},
	volume = {50},
	issn = {0031-9384},
	doi = {10.1016/0031-9384(91)90026-K},
	abstract = {Although the kinematics, mechanics, energetics and mathematics of trot-gallop gait transitions have been discussed in previous papers, no one article has tried to assimilate all the information.  This review paper does this and includes a summary of the relatively new engineering (mathematical) approach to analyzing these transitions developed by Schoner et al. (29).  The paper also presents unpublished data on gait transitions in cats and monkeys, and attempts to develop some general principles pertaining to trot-gallop transitions.  Finally, the paper highlights areas where additional information is needed.},
