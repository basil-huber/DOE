	pages = {4C3--1 --4C3--11},
	file = {IEEE Xplore Abstract Record:/Users/Nicolas/Library/Application Support/Zotero/Profiles/zb908nip.default/zotero/storage/Z3GPSVNG/abs_all.html:text/html;IEEE Xplore Full Text PDF:/Users/Nicolas/Library/Application Support/Zotero/Profiles/zb908nip.default/zotero/storage/FDPNCS32/White et al. - 2012 - An initial examination for verifying separation al.pdf:application/pdf}
