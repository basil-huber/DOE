	year = {2011},
	keywords = {\$importance\_5, \$printed, \$read, acceleration constraints, acceleration control, acceleration proportional control, acceleration-velocity obstacles, Collision avoidance, collision-free navigation, holonomic robots, kinematic constrained nonholonomic robots, mobile robots, multi-robot systems, reciprocal collision avoidance},
	pages = {3475 --3482},
	file = {IEEE Xplore Abstract Record:/Users/Nicolas/Library/Application Support/Zotero/Profiles/zb908nip.default/zotero/storage/WSTA8PCR/cookiedetectresponse.html:text/html;IEEE Xplore Full Text PDF:/Users/Nicolas/Library/Application Support/Zotero/Profiles/zb908nip.default/zotero/storage/A6TSR9TR/van den Berg et al. - 2011 - Reciprocal collision avoidance with acceleration-v.pdf:application/pdf}
