	pages = {620--634},
	file = {ISI Web of Knowledge Record:/Users/Nicolas/Library/Application Support/Zotero/Profiles/zb908nip.default/zotero/storage/4X4TSJK4/full_record.html:text/html;ScienceDirect Full Text PDF:/Users/Nicolas/Library/Application Support/Zotero/Profiles/zb908nip.default/zotero/storage/VZD5MBD2/Santos et Matos - 2011 - Gait transition and modulation in a quadruped robo.pdf:application/pdf;ScienceDirect Snapshot:/Users/Nicolas/Library/Application Support/Zotero/Profiles/zb908nip.default/zotero/storage/NAJG83AK/S0921889011000789.html:text/html}
