	pages = {302 --318},
	file = {IEEE Xplore Abstract Record:/Users/Nicolas/Library/Application Support/Zotero/Profiles/zb908nip.default/zotero/storage/WWMJQQC9/abs_all.html:text/html;IEEE Xplore Full Text PDF:/Users/Nicolas/Library/Application Support/Zotero/Profiles/zb908nip.default/zotero/storage/8D57FIVV/Yang et Meng - 2001 - Neural network approaches to dynamic collision-fre.pdf:application/pdf}
