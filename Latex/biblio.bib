@article{yamamoto_control_????,
	title = {Control of swarm behavior in crossing pedestrians based on temporal/spatial frequency},
	issn = {0921-8890},
	url = {http://www.sciencedirect.com/science/article/pii/S0921889013001073},
	doi = {10.1016/j.robot.2013.06.001},
	abstract = {Abstract 
In the densely-populated urban areas, pedestrian flows often cross each other and congestion is caused. The congestion makes us feel uncomfortable and sometimes leads to pedestrian accidents. To reduce the congestion or the risk of accidents, it is required to control swarm behavior of pedestrian flows. This paper proposes modeling and controlling method of the crossing pedestrian flows. In the social/urban engineering, it is well known that the swarm behavior with a diagonal stripe pattern emerges in the crossing area of the flows. This is a self-organized phenomenon caused by the local collision avoidance effect of the pedestrians. To control macroscopic behavior of the flows, we utilize this self-organized phenomenon. Firstly, we propose the continuum model of the crossing pedestrian flows. In the continuum model, the dynamic change of the congestion in the diagonal stripe pattern is simulated as the density. Secondly, novel control method to improve average flow velocity is proposed based on the model. The proposed method utilizes dynamic interaction between the diagonal stripe pattern and guides, who are moving in the flows. The authors derive the control algorithm through an analysis on the temporal and spatial frequency of the crossing flows. The validity is verified with simulations using the continuum model. Moreover, we apply the proposed method to the particle model, assuming the actual pedestrians.},
	urldate = {2013-06-25},
	journal = {Robotics and Autonomous Systems},
	author = {Yamamoto, Ko and Okada, Masafumi},
	keywords = {Control of swarm behavior, Crossing pedestrian flows, Nonlinear oscillation, Temporal/spatial frequency},
	file = {ScienceDirect Full Text PDF:/Users/Nicolas/Library/Application Support/Zotero/Profiles/zb908nip.default/zotero/storage/BF3R6HVD/Yamamoto et Okada - Control of swarm behavior in crossing pedestrians .pdf:application/pdf;ScienceDirect Snapshot:/Users/Nicolas/Library/Application Support/Zotero/Profiles/zb908nip.default/zotero/storage/MX9I4KWB/S0921889013001073.html:text/html}
}

