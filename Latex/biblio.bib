@article{boussauw_minimum_2011,
	title = {Minimum commuting distance as a spatial characteristic in a non-monocentric urban system: {The} case of {Flanders}},
	volume = {90},
	copyright = {© 2010 the author(s). Journal compilation © 2010 RSAI},
	issn = {1435-5957},
	shorttitle = {Minimum commuting distance as a spatial characteristic in a non-monocentric urban system},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1435-5957.2010.00295.x/abstract},
	doi = {10.1111/j.1435-5957.2010.00295.x},
	abstract = {Abstract
This paper focuses on regional variations in commuting trip lengths by calculating minimum (required) commuting distances, along with excess commuting rates. The study contributes to the excess commuting research framework from a regional perspective, both by stressing the specific characteristics of urban networks with overlapping commute areas, and by putting forward an alternative method for calculating spatially disaggregated values. A case study in the north of Belgium shows that large variations in minimum commuting distances occur. This in turn identifies to a large extent opportunities for shrinking commuting distances by influences such as rising fuel prices, compact urban planning, extreme congestion or dissuasive traffic policies.
Resumen
Este artículo se centra en las diferencias regionales en la longitud del desplazamiento hasta el puesto de trabajo, mediante el cálculo de las distancias mínimas de desplazamiento (requeridas), junto con las tasas de desplazamiento en exceso. El estudio contribuye desde una perspectiva regional al corpus de investigación sobre el exceso de desplazamiento al puesto de trabajo, por un lado haciendo hincapié en las características específicas de las redes urbanas que se solapan con áreas dormitorio y por otro proponiendo un método alternativo para calcular los valores espacialmente desagregados. Un estudio de caso sobre el norte de Bélgica muestra la incidencia de grandes diferencias en las distancias mínimas de desplazamiento. Esto identifica a su vez, en gran medida, una serie de oportunidades para la reducción de las distancias de desplazamiento debido a la influencia del aumento en el precio del combustible, la planificación urbanística compacta, atascos de tráfico extremos, o políticas de tráfico disuasorias.},
	language = {en},
	number = {1},
	urldate = {2014-06-10},
	journal = {Papers in Regional Science},
	author = {Boussauw, Kobe and Neutens, Tijs and Witlox, Frank},
	month = mar,
	year = {2011},
	keywords = {Excess commuting, Flanders, R14, R23, R40, spatial proximity, sustainable spatial development},
	pages = {47--65},
	file = {Snapshot:/Users/Nicolas/Library/Application Support/Zotero/Profiles/zb908nip.default/zotero/storage/R6BXFZT7/abstract.html:text/html}
