@article{bicchi_optimal_2000,
	title = {On optimal cooperative conflict resolution for air traffic management systems},
	volume = {1},
	issn = {1524-9050},
	doi = {10.1109/6979.898228},
	abstract = {We consider optimal resolution of air traffic (AT) conflicts. Aircraft are assumed to cruise within a given altitude layer and are modeled as a kinematic system with constant velocity and curvature bounds. Aircraft cannot get closer to each other than a predefined safety distance. For such a system of multiple aircraft, we consider the problem of planning optimal paths among given waypoints. Necessary conditions for optimality of solutions are derived and used to devise a parametrization of possible trajectories that turns into efficient numerical solutions to the problem. Simulation results for a realistic aircraft conflict scenario are provided. A decentralized implementation of the optimal conflict resolution scheme is introduced that may allow free-flight coordination in a cooperative airspace management scheme. Impact of decentralization on performance and safety is finally discussed with the help of extensive simulations},
	number = {4},
	journal = {IEEE Transactions on Intelligent Transportation Systems},
	author = {Bicchi, A. and Pallottino, L.},
	month = dec,
	year = {2000},
	keywords = {air traffic control, air traffic management systems, altitude layer, cooperative airspace management scheme, decentralized implementation, free-flight coordination, game theory, kinematic system, necessary optimality conditions, Optimal control, optimal cooperative conflict resolution, predefined safety distance, safety, waypoints},
	pages = {221 --231},
	file = {IEEE Xplore Abstract Record:/Users/Nicolas/Library/Application Support/Zotero/Profiles/zb908nip.default/zotero/storage/KNR5TJ2Q/stamp.html:text/html;IEEE Xplore Full Text PDF:/Users/Nicolas/Library/Application Support/Zotero/Profiles/zb908nip.default/zotero/storage/RGXXZI73/Bicchi et Pallottino - 2000 - On optimal cooperative conflict resolution for air.pdf:application/pdf}
